.product_details{
    margin: 20px 0;
    border: solid 1px #DDD;
}
.reconciledDetailsHeaderTitle{
    padding: 20px;
    background: -webkit-linear-gradient(top, #f8f8f8, #eeeeee);
    background: -o-linear-gradient(top, #f8f8f8, #eeeeee);
    background: -ms-linear-gradient(top, #f8f8f8, #eeeeee);
    background: -moz-linear-gradient(top, #f8f8f8, #eeeeee);
    background: linear-gradient(top, #f8f8f8, #eeeeee);
    box-shadow: 0 1px 0px rgba(255, 255, 255, 0.5) inset;
}
.reconciledDetailsHeaderContent{
    padding: 20px;
}
.smallnegative {
    color: #900;
}
.footer{
    margin-bottom: 25px;
}
/*没有变体*/
.Variations_no_message{
    margin: 0 auto;
    width: 50%;
    padding: 50px;
}
.Variations_no_message .alert-inner-info{
    margin-bottom: 15px;
    border:1px solid #bbd3de;
    border-radius: 4px;
}
.Variations_no_message .alert-container{
    border-radius: 4px;
    padding: 14px 18px;
    padding-left: 60px;
    position: relative;
    font-size: 12px;
}
.Variations_no_message .icon-alert{
    background: url(../img/sprite._V285628189_.png) no-repeat;
    background-size: 91%;
    background-position: 0 0;
    height: 25px;
    left: 18px;
    position: absolute;
    top: 11px;
    width: 25px;
}
.Variations_segment .attribute_enum .wide-wrapped{
    padding-top: 7px;
}
.variation_theme-row{
    box-shadow: none;
    border: 0;
}
.delete-tr{
    text-decoration: line-through!important;
    opacity: 0.5!important;
}
input:disabled{
    cursor: default;
}