<script id="showArray" type="text/x-dot-template">
    <tr>
        <td class="a_color relative width17">
            姓名
            <div class="border-delta border-delta-up triangleRight230"></div>
        </td>
        <td class="a_color relative width10">
            货件编号
            <div class="border-delta border-delta-up triangleRight85"></div>
        </td>
        <td class="a_color relative width10">
            创建时间
            <div class="border-delta border-delta-down triangleRight85"></div>
        </td>
        <td class="a_color relative width10">
            上次更新
            <div class="border-delta border-delta-down triangleRight85"></div>
        </td>
        <td class="width5">MSKU</td>
        <td class="width5">已发货</td>
        <td class="width5">已收到</td>
        <td class="a_color relative width14">
            目的地&nbsp;&nbsp;*
            <div class="border-delta border-delta-up triangleRight170"></div>
        </td>
        <td colspan="2">状态</td>
    </tr>
    {{~it:value:index}}
    <tr class="addbottomColor adds" planNo="{{= value.planNo}}">
        <td class="a_color">{{= decodeURIComponent(value.cargoName)}}</td>
        <td class="cargoNo">{{= decodeURIComponent(value.cargoNo)}}</td>
        <td>{{= decodeURIComponent(value.cargoCreateTime)}}</td>
        <td>{{= decodeURIComponent(value.cargoUpdateTime)}}</td>
        <td>1</td>
        <td>{{= decodeURIComponent(value.deliveryNum)}}</td>
        <td>{{= decodeURIComponent(value.signInNum)}}</td>
        <td>
            {{= decodeURIComponent(value.toCity)}}-{{= decodeURIComponent(value.toZipcode)}}
            <br>
            {{= decodeURIComponent(value.toAddress)}}
        </td>
        <td>{{= decodeURIComponent(value.status)}}</td>
        <td>
            <div class="a-spacing-top-small">
                <span class="awesomeButton buttonLarge secondaryLargeButton inner_button">
                    <span class="button_label actionBtn">{{= decodeURIComponent(value.action)}}</span>
                </span>
            </div>
            <div>
                <a href="javascript:void(0)">下载SKU列表</a>
            </div>
        </td>
    </tr>
    {{~}}
</script>
