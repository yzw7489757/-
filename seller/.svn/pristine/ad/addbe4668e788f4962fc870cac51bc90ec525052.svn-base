                        <div class="a-spacing-top-small">
                            <div class="float_left">
                                <a href="./manage_inventory_setNumber.html" class="btn_a">
                                    <span class="awesomeButton buttonLarge secondaryLargeButton inner_button">
                                        <span class="button_label">返回</span>
                                    </span>
                                </a>
                                <span class="awesomeButton buttonLarge secondaryLargeButton inner_button">
                                    <span class="button_label">删除计划</span>
                                </span>
                                <span class="awesomeButton buttonLarge secondaryLargeButton inner_button">
                                    <span class="button_label">复制计划</span>
                                </span>

                            </div>
                            <div class="float_right">
                                <a href="./manage_inventory_labeling_goods.html" class="click_a">
                                    <button class="awesomeButton buttonLarge primaryLargeButton" type="submit">
                                        <span class="button_label saveBtn">继续</span>
                                    </button>
                                </a>
                            </div>
                            <div class="clear"></div>
                        </div>
                    </div>
                </div>
            </li>
        </ol>

