                                    <td>&nbsp;</td>
                                    <td colspan="2"><input type="checkbox"> 显示ASIN/FNSKU</td>
                                    <td>适用于全部</td>
                                    <td colspan="4">
                                        <select name="" id="select">
                                            <option value="all" selected>适用于全部</option>
                                            <option value="seller">卖家</option>
                                            <option value="amazon">亚马逊</option>
                                        </select>
                                    </td>
                                </tr>
                                <tr>
                                    <td>bc001</td>
                                    <td>123</td>
                                    <td>新品</td>
                                    <td>
                                        选择分类
                                    </td>
                                    <td><span class="optionText"></span></td>
                                    <td>20</td>
                                    <td>-</td>
                                    <td>X</td>
                                </tr>
                            </tbody>
                        </table>
                        <div>
                            <div class="float_left">
                                <div class="a-spacing-small">
                                    总计
                                </div>
                                <div class="a-spacing-small">显示<span>1</span>到<span>1</span>件<span>1</span>商品</div>
                            </div>
                            <div class="float_right">
                                <span class="pd20">20</span>
                                <span class="pd20">$0.00</span>
                            </div>
                            <div class="clear"></div>
                        </div>

                    </div>
                    <div>
