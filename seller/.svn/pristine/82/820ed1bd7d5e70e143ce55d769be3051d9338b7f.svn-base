html{
    font-size:13px;
   }
body {
    font-size: 13px;
    line-height: 19px;
    color: #111;
    font-family: "Hiragino Sans GB","Microsoft Yahei",Arial,sans-serif;
}
* {
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
ul{
    margin: 0;
    padding: 0;
}
li{
    list-style-type: none;
    margin: 0;
    padding: 0;
}

a{
    text-decoration: none;
}
.width52{
    width: 52.43%;
}
.width17{
    width: 17.43%;
}
.width8{
    width: 8.68%;
}
.width69{
    width: 69.93%;
}
.overflow_hidden{
    overflow: hidden;
}
.float_left{
    float: left !important;
}
.float_right{
    float: right !important;
}
.inline-block{
    display: inline-block;
}
.box-sizing{
    box-sizing: border-box;
}
.relative{
    position: relative;
}
.pdleft{
    padding-left: 3px;
}
.pdleft5{
    padding-left: 5px;
}
.pdTop5{
    padding-top: 5px;
}
#listingstatus-center {
    padding: 2rem;
}
.a-container {
    min-width: 998px;
    padding: 14px 18px 18px;
    margin: 0 auto;
}
.a-section {
    margin-bottom: 22px;
}
.a-size-large {
    font-size: 21px!important;
    line-height: 1.3!important;
    font-family: "Hiragino Sans GB","Microsoft Yahei",Arial,sans-serif;
}
/* td.a-span12, th.a-span12 {
    width: 104.93%;
} */

h1{
    margin: 0;
    padding: 0;
    color: #e77600!important;
}
.registration title{
    width: 84px;
    height:28px;
    padding-right:3px;
}
a, a:active, a:link, a:visited {
    text-decoration: none;
    color: #0066c0;
}
.a-icon, .a-link-emphasis:after {
    background-image: url(../img/AmazonUIBaseCSS-sprite.png);
    -webkit-background-size: 400px 650px;
    background-size: 400px 650px;
    background-repeat: no-repeat;
    display: inline-block;
    vertical-align: top;
}
.a-icon-popover {
    margin: 5px 0 0 .385em;
    vertical-align: text-top;
    width: 7px;
    height: 5px;
    background-position: -90px -5px;
    opacity: .6;
}
/* td.registration-listingstatus.registration-listingstatus.registration-listingstatus {
    padding-top: .7rem;
    padding-bottom: .7rem;
    vertical-align: middle;
} */
td.registration-listingstatus.registration-listingstatus.padding-left-small {
    padding-left: .5rem;
}
tr.registration-listingstatus.border {
    border-bottom: 1px solid;
    border-color: #E6E6E6;
}
.a-button-inner {
    box-shadow: 0 1px 0 rgba(255,255,255,.6) inset;
    background-color: #fff;
    background-image: none;
    height: auto;
    padding: 5px 10px 5px 11px;
    line-height: 19px;
}
table {
    margin-bottom: 18px;
    border-collapse: collapse;
    width: 100%;
}
.a-text-bold {
    font-weight: 700!important;
}
.pdTop_Bot{
    padding-top: .7rem;
    padding-bottom: .7rem;
}
.pdBot{
    padding-bottom: .7rem; 
}
.submitBtn{
   margin: 0 0 18px 18px; 
}
.submitBtn_big{
    width: 49px;
    height: 31px;
    box-sizing: border-box;
}


.a-button {
    background: #e7e9ec;
    border-radius: 3px;
    border-color: #adb1b8 #a2a6ac #8d9096;
    border-style: solid;
    border-width: 1px;
    display: inline-block;
    text-align: center;
    text-decoration: none!important;
    vertical-align: middle;
}
.a-button-primary {
    background: #f0c14b;
    border-radius: 3px;
    border-color: #adb1b8 #a2a6ac #8d9096;
    border-style: solid;
    border-width: 1px;
    color: #111;
}
p{
    cursor: pointer;
    -webkit-appearance: button;
}
.a-declarative{
    position: relative;
}
#model{
    display: none;
    position: absolute;
    top:20px;
    left:0;
    z-index: 299;
    visibility: visible;
    transform: translateX(-20%);
    width: 440px;
    box-sizing: border-box;
}
#model .a-icon, .a-hires .a-link-emphasis:after {
    background-image: url(../img/AmazonUIBaseCSS-sprite.png);
}
.tiny a:hover,.moreMessage:hover{
    text-decoration: underline;
    color:#c45500;
    text-decoration-color: #c45500;
}
.a-icon-close {
    width: 10px;
    height: 9px;
    background-position: -297px -5px;
    opacity: .64;
    position: absolute;
    top: 12px;
    right: 12px;
    cursor: pointer;
}

em, i {
    font-style: italic;
}
.a-popover-inner {
    padding: 14px 18px;
    text-align: left;
    overflow-x: hidden;
}
#model>div{
    overflow: hidden;
    border: 1px solid #cdcdcd;
    border-color: rgba(0,0,0,.2);
    height: 100%;
    width: 100%;
    padding:8px;
    border-radius: 4px;
    box-shadow: 0 2px 4px rgba(0,0,0,.13);
    background-color: #fff;
}
.a-spacing-medium, .a-ws .a-ws-spacing-medium {
    margin-bottom: 18px!important;
}
a, a:active, a:link, a:visited {
    text-decoration: none;
    color: #0066c0;
}

#model .popup>span{
    display: block;
    width: 0;
    height: 0;
    border-width: 0 8px 8px;
    border-style: solid;
    border-color: transparent transparent #ccc;
    position: absolute;
    top: -8px;
    left: 30%;
    margin-left: -9px;
}
#model .popup>span>em{
    display: block;
    width: 0;
    height: 0;
    border-width: 0 8px 8px;
    border-style: solid;
    border-color: transparent transparent #fff;
    position: absolute;
    top: 1px;
    left: -8px;
}