/* nav*/

header{
  box-sizing: border-box;
}
.clearable {
  height: 38px;
  padding: 6px 2.5% 0;
  border-bottom: 1px solid #ccc;
  background-image: url(../img/ResponseXueb.png);
  background-position: -9999px -9999px;
  background-repeat: no-repeat;
  background-color: #f3f3f3;
  font-size: 12px;
  min-width: 300px;
  z-index: 100;
  height: 45px;
}

.qi {
    margin-left: 25px;
    margin-top: 5px;
    font-size: 20px;
}
.display_block{
  display:block  !important;
}
.opacity_visibility{
  opacity: 1 !important;
  visibility:visible  !important;
}
.sc-sub-nav-right-hover:hover{
  background: #ccc
}
.sc-sub-nav-right-hover:first-child{
  border-bottom: 1px solid #ccc;
}
.sc-sub-nav-right>li>a{
  color: #333;
}
.a-span6 {
  width: 52.43%;
  float: none !important;
}

.a-container {
  min-width: 998px;
  padding: 14px 18px 18px;
  margin: 0 auto;
}
.a-container-footer{
  min-width: 998px;
  margin: 0 auto;
  padding-top: 18px;
}
.a-normal {
  margin-bottom: 18px;
  border-collapse: collapse;
  width: 100%;
}

.a-normal tbody tr td {
  vertical-align: top;
}

.overflow {
  overflow: hidden;
}

tbody {
  display: table-row-group;
  vertical-align: middle;
  border-color: inherit;
}

tbody tr {
  display: table-row;
  vertical-align: inherit;
  border-color: inherit;
}

.a-span3 {
  width: 26.18%;
  float: none !important;
}

.pdlr17 {
  padding-left: 17px;
  padding-right: 17px;
}

#sc-masthead {
  position: relative;
  width: 100%;
}

#sc-logo-top {
  position: relative;
  left: 0;
  top: 0;
}

#sc-logo-top img {
  width: 165px;
  height: 34px;
}

#sc_logo_top_image.sc-unified {
  padding: 6px 3px 0;
}

#sc-navtabs {
  /* overflow: hidden; */
  height: 38px;
  float: left;
  width: 380px;
  margin-left: 10px;
}

#sc-snes {
  float: left;
  position: relative;
  z-index: 102;
  height: 38px;
  margin: 0 12px -1px 3px;
}

#sc-navtabs>ul {
  /* overflow: hidden; */
  float: left;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}

@font-face {
  font-family: 'iconfont';
  /* project id 715211 */
  src: url('//at.alicdn.com/t/font_715211_9quj7rrovbb.eot');
  src: url('//at.alicdn.com/t/font_715211_9quj7rrovbb.eot?#iefix') format('embedded-opentype'), url('//at.alicdn.com/t/font_715211_9quj7rrovbb.woff') format('woff'), url('//at.alicdn.com/t/font_715211_9quj7rrovbb.ttf') format('truetype'), url('//at.alicdn.com/t/font_715211_9quj7rrovbb.svg#iconfont') format('svg');
}

#sc-navtabs>ul:hover {
  float: left;
}

#sc-navtabs>ul>li {
  float: left;
  position: relative;
}

.sc-sub-nav {
  display: none; 
  position: absolute;
  left:0;
  top:35px;
  padding: 2px 0;
  background-color: #fff;
  border-top: 3px solid #333;
  -webkit-box-shadow: 0 3px 3px rgba(0,0,0,0.2);
  -moz-box-shadow: 0 3px 3px rgba(0,0,0,0.2);
  box-shadow: 0 3px 3px rgba(0,0,0,0.2);
}
.sc-sub-nav>li{
  white-space: nowrap;
  display: block;
  margin: 0 3px;
  padding: 1px 0;
  background-image: none;
  padding-left: 5px;

}
.sc-sub-nav>li>a{
  color: #333;
  display: block;
  padding: 7px 6px;
}
.sc-nav-arrow{
  display: block;
  position: absolute;
  margin: 0;
  left: 50%;
  margin-left: -6px;
  bottom: 0;
  height: 0;
  padding: 0;
  border-left: 6px solid transparent;
  border-right: 6px solid transparent;
  border-bottom: 6px solid #333;
  z-index: 102;
  opacity: 0;
  visibility: hidden;
}
.sc-nav-arrow-right{
  display: block;
  position: absolute;
  margin: 0;
  right: 6px;
  top: 29px;
  margin-left: -6px;
  bottom: 0;
  height: 0;
  padding: 0;
  border-left: 6px solid transparent;
  border-right: 6px solid transparent;
  border-bottom: 6px solid #333;
  z-index: 102;
  opacity: 0;
  visibility: hidden;
}
#sc-masthead * {
  font-family: Arial, Helvetica, sans-serif;
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}

#sc-logo-top {
  height: 38px;
  margin-bottom: -1px;
  font-size: 12px;
  float: left;
}

#sc-top-nav-root {
  font-size: 12px;
  list-style: none;
  /*width: 337px;*/
  margin: 0;
  padding: 0;
}

.sc-level1 .sc-tab-a {
  display: block;
  padding: 17px 0 18px;
  text-decoration: none;
  font-weight: bold;
  color: #5c5c5c !important;
  line-height: 1px;
  position: relative;
  height: 1px;
  border-bottom: 3px solid transparent;
  text-transform: uppercase;
}

.sc-level1 {
  position: relative;
  float: left;
  display: block;
  padding: 0 20px 0 0;
  margin: 0 1px 0 0;
  z-index: 101;
  font-size: 13px;
}

/*卖家消息*/

#right_nav {
  float: right;
  color: #777;
  position: relative;
  z-index: 102;
}

#sc-quicklinks {
  list-style: none;
  padding: 0;
  margin: 0;
}


.sc-quick-txt {
  border-right: 1px solid #999;
  padding-right: 5px;
}

@media screen and (max-width: 1296px) {
  body {
      /* background-color:lightblue; */
  }
  #sc-navtabs {
      position: absolute;
      top: 45px;
  }
  .container {
      margin-top: 45px;
  }
  .clearable {
      height: 81px;
  }
  .qi {
      display: none;
  }
  .sc-global-search-input{
      display: none;
  }
  .nav_right_opt p{
      display: none;
  }
  .nav_right_opt_select_p{
      display: none;
  }
}
/* 搜索框 */
@media only screen and (max-width:1340px) {
  .sc-global-search-p{
      display: block;
      background-image: url(../img/ResponseXueb.png);
      background-position: -39px -24px;
      background-repeat: no-repeat;
      text-indent: -9999px;
      height: 23px;
      width: 18px;
      margin: 4px 3px 9px;
      cursor: pointer;
  }
  .sc-global-search-input{
      display: none;
  }
}
/* 双向等号 */
@media only screen and (max-width:1250px) {
  .nav_right_opt_select{
      display: none;    
  }
  .nav_right_opt_img>div{
      display: none; 
  }
  .nav_right_opt_select_p{
      display: block;
      background-image: url(../img/ResponseXueb.png);
      background-position: -57px -24px;
      background-repeat: no-repeat;
      text-indent: -9999px;
      height: 23px;
      width: 20px;
      margin: 4px 3px 9px;
      cursor: pointer;
  }

}
