/*搜索*/

#sc-global-search-form {
  margin-top: 4px;
}
#sc-global-search-form>.sc-global-search-input>input {
  width: 162px;
  outline: medium;
  float: left;
  height: 24px !important;
  margin-left: 9px;
  background-color: #fff;
  border: 1px solid #d0d0d0;
  border-top: 1px solid #bbb;
  font-size: 12px;
}

#sc-global-search-form>.sc-global-search-input>input:focus {
  /* outline: medium; */
  /* border-color: transparent;   */
  box-shadow: none;
}

#sc-global-search-form>.sc-global-search-input>div {
  height: 24px;
  width: 26px;
  float: left;
  border-radius: 1px;
  background-image: linear-gradient(rgb(224, 224, 224) 0px, rgb(198, 198, 198) 100%);
}

#sc-global-search-form>.sc-global-search-input>div>img {
  width: 100%;
  display: block;
  overflow: hidden;
  border-radius: 0 3px 3px 0;
  border: 1px solid #c6c6c6;
  border-bottom: 1px solid #bbb;
  border-left: 0;
  height: 24px;
  width: 25px;
  -webkit-box-shadow: inset 0 1px 0 rgba(255,255,255,0.3);
  -moz-box-shadow: inset 0 1px 0 rgba(255,255,255,0.3);
  box-shadow: inset 0 1px 0 rgba(255,255,255,0.3);
  background: #e0e0e0;
  background: -moz-linear-gradient(top,#e0e0e0 0,#c6c6c6 100%);
  background: -webkit-linear-gradient(top,#e0e0e0 0,#c6c6c6 100%);
  background: -o-linear-gradient(top,#e0e0e0 0,#c6c6c6 100%);
  background: -ms-linear-gradient(top,#e0e0e0 0,#c6c6c6 100%);
  background: -webkit-gradient(linear,left top,left bottom,color-stop(0%,#e0e0e0),color-stop(100%,#c6c6c6));
  background: linear-gradient(to bottom,#e0e0e0 0,#c6c6c6 100%);
  /* margin-top: -23px; */
}

#selectzg{
  margin-right: 16px
}

