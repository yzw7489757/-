body {
    font-size: 13px;
    line-height: 19px;
    color: #111;
    font-family: Arial,sans-serif;
}
.a-container {
    min-width: 998px;
    padding: 14px 18px 18px;
    margin: 0 auto;
}
.a-container>ul {
    /* overflow: hidden; */
    border-bottom: 1px solid #ddd;
    height: 41px;
    box-sizing: border-box;
    width: 1200px;
}
.a-container>ol{
    width: 1200px;   
}
.a-container>ul>li {
    float: left;
}
.a-container>ol>li{
    display: none;
}
.a-container>ol>li:nth-child(1){
    display: block;
}
.a-container>ul>li>a {
    display: block;
    padding: 10px;
    background: #fff;
    box-sizing: border-box;
}
.active {
    border-top: 3px solid #e77600;
    border-left: 1px solid #ddd;
    border-right: 1px solid #ddd;
    height: 41px;
    color: #c45500 !important;
    font-weight: 700;
}
.a-container .containerHeader{
    font-weight: 700;
    font-size: 21px;
    line-height: 1.3;
    color: #e77600;
    padding-right: 5px;
    font-family: "Hiragino Sans GB","Microsoft Yahei",Arial,sans-serif;
}
ol li h2{
    font-weight: normal !important;
}
ol li button{
    padding: 6px 8px;
    background: #f0c14b;
    border-color: #a88734 #9c7e31 #846a29;
    color: #111; 
    border-radius: 5px;
    cursor: pointer;
    border-style: solid;
    border-width: 1px;
}
.layout .layoutLeft{
    width: 40%;
}
.layout .layoutright{
    width: 60%;
}
.layout .layoutright select{
    width: 70% !important;
    display: block;
}
.width33{
    width: 33.3%
}
.pdTop10{
    padding: 10px 0;
}
.marLeft88{
    margin-left: 165px;
}
.marRight5{
    margin-right: 5px;
}
.border_bottom{
    border-bottom:1px solid #ddd;
}
.pdLeft33{
    padding-left: 33%;
}

.screenTable{
    width: 100%;
    background-image: linear-gradient(rgb(248, 248, 248), rgb(238, 238, 238));
    border-radius: 4px 4px 0 0;
    margin-bottom: 5px;
}
.screenTable tr td{
    border: 1px solid rgb(8, 8, 8);
    padding: 5px;
    box-sizing: border-box;
}
.olPay{
    padding: 15px;
    margin-left: 15px;
}
.olPay li{
    list-style-type: disc;
    
}
input[type=text]{
    height: 21px;
    width: 200px;
}
.dateRange{
    height: 80px;
    padding: 14px 8px;
    background: #d4f1f7;
    border: 1px solid rgb(8, 8, 8);
    border-radius: 5px;
    box-sizing: border-box;
}
.colorYE{
    color:#f74e4e;
}
.colorXJ{
    color: #60a4ec;
}
.colorZJ{
    color: #e69022
}