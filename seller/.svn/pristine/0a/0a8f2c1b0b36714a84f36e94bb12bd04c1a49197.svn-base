.pageLoaded .cb-serviceprovider-breadcrumb {
    float: right;
    font-size: 15px;
    margin-top: 10px;
    padding-right: 15px;
    padding-left: 15px;
    min-height: 1px;
}

@media (min-width: 1200px) {
    .col-lg-8 {
        width: 66.66667%;
    }
}

.cb-serviceprovider-logodiv {
    margin-bottom: 3px;
    padding-left: 13px;
}
.cb-serviceprovider-imageouterbox {
    float: left;
    margin-right: 18px;
    margin-top: 0px;
    margin-bottom: 30px;
    margin-left: 15px;
}
.cb-serviceprovider-imagediv {
    width: 84px;
    height: 84px;
    display: table-cell;
    vertical-align: middle;
    border: 1px solid #303942;
}
.cb-serviceprovider-imagediv img{
    width: 100%;
    height: 100%;
}
.cb-serviceprovider-namediv {
    margin-top: 0px;
    margin-bottom: 30px;
}
.cb-serviceprovider-namediv .cb-serviceprovider-name {
    margin-top: 0px;
    font-size: 30px;
    color: #f8991d;
}
.cb-serviceprovider-namediv  .cb-serviceprovider-colon {
    padding-right: 10px;
    font-size: medium;
    display: inline-block;
}
.cb-serviceprovider-aggregateRow {
    color: #009786;
    float: right;
    margin-bottom: 10px;
    padding-right: 30px;
}
.btn {
    display: inline-block;
    margin-bottom: 0;
    font-weight: normal;
    text-align: center;
    vertical-align: middle;
    touch-action: manipulation;
    cursor: pointer;
    background-image: none;
    border: 1px solid transparent;
    white-space: nowrap;
    padding: 6px 12px;
    font-size: 14px;
    line-height: 1.42857;
    border-radius: 4px;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}
.cb-browsetool-button, .cb-browsetool-button:visited {
    float: right;
    background-color: #0097d6;
    color: #FFF;
    border-radius: 1px;
}
.cb-servicerequest-button, .cb-servicerequest-button:visited, .cb-servicerequest-button:hover, .cb-servicerequest-button.btn-default:hover, .cb-servicerequest-button.btn-default:focus, .cb-servicerequest-button.btn-default.focus, .cb-servicerequest-button.btn-default:active, .cb-servicerequest-button.btn-default.active, .cb-servicerequest-button.open>.btn-default.dropdown-toggle {
    background-color: #FF9900;
    color: #FFF;
    border-radius: 1px;
    margin: 10px;
    float: none !important;
}
.cb-serviceprovider-aggregateRow .cb-browsetool-button {
    margin-top: 10px;
}