            <nav class="navbar navbar-default land">
                <a href="">
                    登陆
                </a>
            </nav>
            <nav class="navbar navbar-default land">
                <select id="language">
                    <option value="string:it_IT" label="Italian italiano">Italian italiano</option>
                    <option value="string:ko_KR" label="Korean 한국어">Korean 한국어</option>
                    <option value="string:en_US" label="English">English</option>
                    <option value="string:zh_CN" label="Chinese 中文" selected="selected">Chinese 中文</option>
                    <option value="string:es_ES" label="Spanish Español">Spanish Español</option>
                    <option value="string:fr_FR" label="French Français">French Français</option>
