.pull-left{float:left;}
.pull-right{float:right;}
.clearfix:before,
.clearfix:after {display: table;content: " ";}
.clearfix:after {clear: both;}
a:hover {
    text-decoration: none;
    cursor: pointer;
    color: #c45500;
}
.block{
	display: block;
}
.a-row {
    width: 100%;
}
.amazon-manage-order-wrap{
	width: 100%;
}
.amazon-manage-order-wrap h1{
	margin: 0px;
}
#amazon-manage-order{
	padding: 0px 2.5%;
}
.amazon-manage-order-header{
	margin-top: 26px;
}
#search-order-input{
    width: 310px;
    border-radius: 2px 0 0 2px;
    margin-left: 8px;
}
#search-order-submit{
	margin-left: -1px;
}
.a-button {
    background: #e7e9ec;
    border-radius: 3px;
    border-color: #ADB1B8 #A2A6AC #8D9096;
    border-style: solid;
    border-width: 1px;
    cursor: pointer;
    display: inline-block;
    padding: 0;
    text-align: center;
    text-decoration: none!important;
    vertical-align: middle;
}
.a-button-dark, .a-button-search {
    background: #444C55;
    border-color: #3d444c #2f353b #2c3137;
    color: #fff;
}
.a-button-inner {
    display: block;
    position: relative;
    overflow: hidden;
    height: 29px;
    box-shadow: 0 1px 0 rgba(255,255,255,.6) inset;
    border-radius: 2px;
}
.a-button .a-button-inner {
    background: #eff1f3;
    background: -webkit-linear-gradient(top,#f7f8fa,#e7e9ec);
    background: linear-gradient(to bottom,#f7f8fa,#e7e9ec);
}
.a-button-dark .a-button-inner, .a-button-search .a-button-inner {
    background: #5b626a;
    background: -webkit-linear-gradient(top,#72787F,#444C55);
    background: linear-gradient(to bottom,#72787F,#444C55);
    box-shadow: 0 1px 0 rgba(255,255,255,.15) inset;
    text-shadow: 0 -1px 0 rgba(0,0,0,.25);
}
.a-button-text {
    background-color: transparent;
    border: 0;
    display: block;
    font-size: 13px;
    line-height: 29px;
    margin: 0;
    outline: 0;
    padding: 0 10px 0 11px;
    text-align: center;
    white-space: nowrap;
}
.a-button-dark .a-button-text, .a-button-search .a-button-text {
    font-weight: 700;
    color: #fff;
}
.a-button-dropdown .a-button-text {
    padding-right: 22px;
    text-align: left;
    overflow: hidden;
    text-overflow: ellipsis;
    position: relative;
    z-index: 10;
    color: #111;
}
.a-icon-dropdown {
    width: 5px;
    height: 8px;
    background-position: -65px -5px;
}
.a-button .a-icon-dropdown {
    position: absolute;
    top: 10px;
    right: 9px;
}
.dropdown-menu{
    overflow-y: auto;
    border: 1px solid #cdcdcd;
    border-color: rgba(0,0,0,.2);
    border-radius: 4px;
    box-shadow: 0 2px 4px rgba(0,0,0,.13);
}
.a-dropdown-link {
    padding: 2px 12px 1px 13px;
    display: block;
    white-space: nowrap;
    outline: 0;
    border: 1px solid transparent;
    margin-left:0px;
}
.a-dropdown-link.active {
    background-color: #f3f3f3;
    border-color: #e7e7e7;
    border-top-color: #e7e7e7;
    border-left-color: #e77600!important;
}
#search-order-condition .dropdown-menu{
	top: -3px;
	left: -1px;
	height: 169px;
    width: 131px;
}
.amazon-manage-order-content .tabs-container{
    border-width: 0 0 1px;
    border-style: solid;
    border-color: #ccc;
    border-radius: 0;
    margin-top: 10px;
}
.a-color-tertiary {
    color: #767676!important;
}
.spa-tab-container{
	font-size: 0px;
	margin: 0px;
	padding: 0px;
}
.spa-tab-container li{
	padding: 0 0 12px;
    margin-right:35px;
}
.spa-tab-container li a{
    color: #111;
    font-weight: 100;
    text-decoration: none;
}
.spa-tab-container li a :hover{
	text-decoration: none;
}
.spa-tab-container li a h4{
	margin: 0px;
}
.spa-highlight{
	color: #e77600;
	margin-bottom: 4px;
}
.spa-tab-container li.active {
    border-width: 0 0 2px;
    border-style: solid;
    border-color: #e77600;
    border-radius: 0;
} 
.spa-tab-container li.active h4{
	 font-weight: bold;
}  
#amazon-manage-order-quick-filters-bar{
    margin: 10px 0px;
    border-width: 0 0 1px;
    border-style: solid;
    border-color: #ccc;
    border-radius: 0;
    height: 30px;
    -webkit-box-shadow: 0 4px 6px -6px #ccc;
    -moz-box-shadow: 0 4px 6px -6px #ccc;
    box-shadow: 0 4px 6px -6px #ccc;
}
.myo-spa-quick-filter {
    padding: 0 0 6px;
    margin: 0 0 0 18px;
    display: inline-block;
}
.a-text-bold{
	font-weight: bold;
}
.myo-filter-topbar-badge {
    margin: 0 8px 5px 0;
    padding: 4px 8px;
    display: inline-block;
    border-radius: 1px;
    border: 1px solid #ddd;
    position: relative;
    cursor: pointer;
}
.myo-filter-topbar-badge .margin-right {
    margin-right: 15px;
}
.delete-condition {
    position: absolute;
    right: 12px;
    top: 6px;
}
.delete-condition:after, .delete-condition:before {
    position: absolute;
    content: " ";
    height: 14px;
    width: 2px;
    background-color: #333;
}
.delete-condition:before {
    transform: rotate(45deg);
}
.delete-condition:after {
    transform: rotate(-45deg);
}
 
.myo-filter-topbar-badge.blue {
    color: #0066c0;
}
.myo-filter-topbar-badge.red{
	color: #b12704;
}
.a-popover-preloadv {
    background-color: #fff;
    padding: 14px 18px;
    text-align: left;
    overflow-x: hidden;
    color: #333;
    margin-top: 8px;
    white-space: nowrap;
}
.myo-sqf-heading {
    margin: 0 0 16px;
    white-space: nowrap;
}
.a-size-medium {
    font-size: 17px;
    line-height: 1.255;
}
.myo-sqf-body-shaded {
    padding: 8px 0;
}
.myo-sqf-body-shaded li {
    list-style-type: none;
    margin: 8px 0 -4px;
}
.myo-sqf-footer button,.modal-footer button,.filter-selection button{
	height: 29px;
	padding: 0px 10px;
	margin-left: 8px;
}
.a-button-primary {
    background: #f0c14b;
    border-color: #a88734 #9c7e31 #846a29;
    color: #111;
}
.amazon-manage-order-layout{
	margin-top:10px;
}
.amazon-manage-order-sidebar{
	margin-right: 2%;
    min-height: 1px;
    overflow: visible;
    width: 14.948%;
}
.myo-well {
    border-radius: 2px;
    border: 1px solid #ddd;
    background-color: #f3f3f3;
    padding: 18px;
}
#myo-filters-form {
    margin-top: 18px;
    margin-bottom: 0;
}
.myo-well .myo-options {
    border-width: 1px 0;
    border-radius: 0;
    border-style: solid;
    border-color: #ddd;
    margin: 6px 0 18px;
    padding: 6px 0;
}
.myo-well .myo-options label{
	display: block;
	font-weight: normal;
}
.amazon-manage-order-main-content{
	width: 82.948%;
}
.a-spacing-mini{
    margin-bottom: 6px;
}
.total-orders-heading {
    font-size: 24px;
    font-weight: 700;
    display: inline-block;
    vertical-align: middle;
    margin-left: 10px;
}
.a-dropdown-container{
	margin-left: 8px;
}
.a-dropdown-container .dropdown-menu{
	top: -3px;
	left: -1px;
	max-height: 169px;
	overflow-y: scroll;
    width: auto;
}
.no-data{
	margin: 10px 0px;
}
 
.a-horizontal-stripes td,.a-horizontal-stripes th{
    height: auto !important;
    max-width: 200px;
    text-align: center;
}
.a-horizontal-stripes td p{
	margin-bottom: 0px;
}
.a-horizontal-stripes  .blue-color{
	color: #0066c0;
	font-weight: bold;
	margin-bottom: 8px;
	display: block;
	text-decoration: underline;
	
}
.order-inforamtion  .product-information p{
	margin-bottom: 4px;
}
.a-horizontal-stripes .action button{
	padding: 4px 6px;
	margin: 6px auto;
}
.order-inforamtion{
	text-align: left;
}
.order-action button{
	margin-bottom: 4px;
}
#amazon-manage-order-standing-by .header-filters{
	margin-top: 15px;
}
.filter-selection p{
	margin-bottom:0px;
	margin-right: 8px;
	font-size: 14px;
	line-height: 29px;
}
.filter-selection button{
	height: 29px;
	margin-right: 10px;
}
/*重新定义弹窗样式*/
.amazon-manage-order-modal{
	
}
.amazon-manage-order-modal .modal-dialog{
	width: 947px;
}
.amazon-manage-order-modal .modal-header{
    position: relative;
    text-align: left;
    background: #f2f2f2;
    background: #f1f1f1;
    background: -webkit-linear-gradient(top,#f7f7f7,#eaeaea);
    background: linear-gradient(to bottom,#f7f7f7,#eaeaea);
    border-radius: 4px 4px 0 0;
    border-bottom: 1px solid #cdcdcd;
    border-bottom-color: rgba(0,0,0,.2);
    box-shadow: 0 1px 0 rgba(255,255,255,.5) inset, 0 -1px 0 rgba(255,255,255,.4) inset;
    padding: 0 14px;
    margin: 0;
    z-index: 10;
}
.amazon-manage-order-modal .modal-header h4{
    min-height: 35px;
    line-height: 16px;
    padding: 10px 0 9px;
    font-size: 13px;
    font-weight: 700;
    margin-right: 20px;
}
.amazon-manage-order-modal .modal-header .close{
	height: 36px;
}
.orders-table-preference-group-set{
	margin: 1%;
}
.orders-table-preference-group-title {
    font-weight: 700;
    margin-bottom: .5em;
}
.orders-table-preference-group label {
    font-weight: 400;
}
.orders-table-preference-group-title-container {
    margin-bottom: .75em;
}
.a-button.a-button-focus, .a-button:focus,.a-button:hover{
    outline: 0;
    border-color: #e77600;
    box-shadow: 0 0 3px 2px rgba(228,121,17,.5);
}