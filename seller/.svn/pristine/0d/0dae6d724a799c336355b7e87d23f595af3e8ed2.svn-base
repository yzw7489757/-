body{
    font-family: 'Hiragino Sans GB','Microsoft Yahei',Arial,sans-serif;
    font-size: 13px;
    line-height: 19px;
    color: #111;
}
.madagascar-main-body {
    padding: 0 18px;
}
.madagascar-main-body h3 {
    font-weight: 700;
    font-size: 17px;
    line-height: 1.255;
    color: #c45500 !important;
    text-align: left !important;
    margin-top: 18px !important;
}
.madagascar-main-body ul{
    padding-left: 30px;
    margin-top: 10px;
}
.madagascar-main-body ul li{
    list-style: disc;
}
.madagascar-main-body .info{
    margin-top: 10px;
}
.madagascar-main-body .table{
    border-radius: 10px;
    background-color:#ddd;
    padding: 10px;
    /* margin-top: 20px; */
}
.madagascar-main-body table{
    width: 100%;
    margin-bottom: 10px;
    
}
.madagascar-main-body .table a{
    text-decoration: underline;
}
.madagascar-main-body table tr td{
    padding: 5px;
}
.a-form-label {
    font-weight: 700;
}
