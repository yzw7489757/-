.model-wrapper {
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    z-index: 107;
}
.model-wrapper-bg {
    position: fixed;
    overflow-y: auto;
    width: 100vw;
    height: 100vh;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background: rgba(0,0,0,.4);
}
.model-content {
    position: absolute;
    z-index: 106;
    border: 1px solid #ddd;
    border-radius: 5px;
    background: #fff;
    left: 50%;
    top: 100px;
    transform: translateX(-50%);
}

