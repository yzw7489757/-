.scui-alert {
    border-radius: 4px;
    background-color: white;
    border: 1px solid #ddd;
    line-height: 18px;
    margin: 1.5em auto;
    width: 95%;
}
.scui-alert-error {
    border-color: #c40000;
}
.scui-alert-error>.scui-alert-inner:only-child {
    -webkit-box-shadow: 0 0 0 4px #fcf4f4 inset;
    -moz-box-shadow: 0 0 0 4px #fcf4f4 inset;
    box-shadow: 0 0 0 4px #fcf4f4 inset;
    padding-left: 63px;
}
.scui-alert .scui-alert-inner {
    padding: 14px 18px 12px 60px;
    border-radius: 4px;
    position: relative;
    text-align: left;
}
.scui-alert-error .scui-alert-icon {
    background-position: -248px -35px;
}

.scui-alert .scui-alert-icon {
    background-image: url(../img/aui_sprite_0037-1x._V1_.png);
    background-repeat: no-repeat;
    background-size: 400px 600px;
    display: inline;
    vertical-align: top;
    width: 30px;
    height: 27px;
    position: absolute;
    top: 11px;
    left: 18px;
    _left: -45px;
}
.scui-alert-error h4 {
    color: #c40000;
}
.scui-alert h4 {
    font-size: 16px;
    font-weight: normal;
    line-height: 1.255;
    margin: 0;
    padding: 0 0 5px 0;
}
