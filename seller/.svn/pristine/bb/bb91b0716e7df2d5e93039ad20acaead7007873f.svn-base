                                    </div>
                                    <hr class="a-divider-normal">
                                    <div class="addressList-size" id="addressList">
                                        <div id="addTmpl">
                                        </div>
                                        <script id="addArray" type="text/x-dot-template">
                                            {{~it:value:index}}

                                            <div class="add pd_margin_5" data-id="{{= value.method_id}}">
                                                <div class="itemRadio">
                                                    {{? value.status == true}}
                                                    <input type="radio" class="radioBtn" checked> {{??}}
                                                    <input type="radio" class="radioBtn"> {{?}}

                                                </div>
                                                <label class="itemAddress ">
                                                    <span class="a-form-label">
                                                        {{= decodeURIComponent(value.name)}}

                                                    </span>
                                                    <span>{{= decodeURIComponent(value.name)}}, {{= decodeURIComponent(value.city)}},{{=
                                                        decodeURIComponent(value.province)}}, {{= decodeURIComponent(value.zipcode)}},
                                                        {{= decodeURIComponent(value.country)}}</span>
                                                    <div class="actionButton">
                                                        <a class="editShipFromAddress">
                                                            编辑
                                                        </a>
                                                        {{? value.status == true}}
                                                        <a href="javascript:void(0)" class="selectColor">删除</a>
                                                        {{??}}
                                                        <a>删除</a>
                                                        {{?}}
                                                    </div>
                                                </label>

                                            </div>
                                            {{~}}
                                        </script>
                                    </div>
                                    <div>
                                        <a class="add_new_address">+ 添加新地址</a>
                                    </div>
