  function today() {
    var today = new Date();
    var h = today.getFullYear(); // 年
    var m = today.getMonth() + 1; // 月
    var d = today.getDate(); // 日
    m = m > 9 ? m : '0' + m
    d = d > 9 ? d : '0' + d
    return m + "-" + d + "-" + h;
  }
