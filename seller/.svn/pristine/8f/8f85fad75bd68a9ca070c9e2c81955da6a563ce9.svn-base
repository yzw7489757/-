  // 气泡显示与隐藏
  function showText(bubble_img, bubble_text) {
    var timer = null;
    $(bubble_img).hover(
      function () {
        $(bubble_text).show();
      },
      function () {
        timer = setTimeout(function () {
          $(bubble_text).hide();
        }, 10);
        $(bubble_text).hover(
          function () {
            clearInterval(timer);
          },
          function () {
            timer = setTimeout(function () {
              $(bubble_text).hide();
            }, 10);
          }
        );
      }
    );
  }
