.pageLoaded .cb-serviceprovider-breadcrumb {
    float: right;
    font-size: 15px;
    margin-top: 10px;
    padding-right: 15px;
    padding-left: 15px;
    min-height: 1px;
}

@media (min-width: 1200px) {
    .col-lg-8 {
        width: 66.66667%;
    }
}

.cb-serviceprovider-logodiv {
    margin-bottom: 3px;
    padding-left: 13px;
}

.cb-serviceprovider-imageouterbox {
    float: left;
    margin-right: 18px;
    margin-top: 0px;
    margin-bottom: 30px;
    margin-left: 15px;
}

.cb-serviceprovider-imagediv {
    width: 84px;
    height: 84px;
    display: table-cell;
    vertical-align: middle;
    border: 1px solid #303942;
}

.cb-serviceprovider-imagediv img {
    width: 100%;
    height: 100%;
}

.cb-serviceprovider-namediv {
    margin-top: 0px;
    margin-bottom: 30px;
}

.cb-serviceprovider-namediv .cb-serviceprovider-name {
    margin-top: 0px;
    font-size: 30px;
    color: #f8991d;
}

.cb-serviceprovider-namediv .cb-serviceprovider-colon {
    padding-right: 10px;
    font-size: medium;
    display: inline-block;
}

.cb-serviceprovider-aggregateRow {
    color: #009786;
    float: right;
    margin-bottom: 10px;
    padding-right: 30px;
}

.btn {
    display: inline-block;
    margin-bottom: 0;
    font-weight: normal;
    text-align: center;
    vertical-align: middle;
    touch-action: manipulation;
    cursor: pointer;
    background-image: none;
    border: 1px solid transparent;
    white-space: nowrap;
    padding: 6px 12px;
    font-size: 14px;
    line-height: 1.42857;
    border-radius: 4px;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}

.cb-browsetool-button,
.cb-browsetool-button:visited {
    float: right;
    background-color: #0097d6;
    color: #FFF;
    border-radius: 1px;
}

.cb-servicerequest-button,
.cb-servicerequest-button:visited,
.cb-servicerequest-button:hover,
.cb-servicerequest-button.btn-default:hover,
.cb-servicerequest-button.btn-default:focus,
.cb-servicerequest-button.btn-default.focus,
.cb-servicerequest-button.btn-default:active,
.cb-servicerequest-button.btn-default.active,
.cb-servicerequest-button.open>.btn-default.dropdown-toggle {
    background-color: #FF9900;
    color: #FFF;
    border-radius: 1px;
    margin: 10px;
    float: none !important;
}

.cb-serviceprovider-aggregateRow .cb-browsetool-button {
    margin-top: 10px;
}

@media (min-width: 1200px) {
    .col-lg-3 {
        width: 25%;
    }
}

@media (min-width: 1200px) {
    .col-lg-9 {
        width: 75%;
    }
}

.col-lg-3,
.col-lg-6,.col-lg-9 {
    padding-left: 15px;
    padding-right: 15px;
}
.cb-serviceprovider-pageDetail {
    padding-left: 23px;
}
.cb-serviceprovider-box-left {
    background-color: #f4f5f5;
    padding-top: 20px;
    padding-left: 28px;
    padding-bottom: 20px;
    margin-bottom: 10px;
    font-size: 15px;
    margin-left: -15px;
    word-wrap: break-word;
}

.cb-serviceprovider-box-left h4 {
    margin-bottom: 30px;
    font-size: 20px;
    font-weight: 700;
}

ul,
ol {
    margin-top: 0;
    margin-bottom: 10px;
}

.cb-serviceprovider-overview-heading {
    color: #c45500;
    white-space: pre-wrap;
}

.cb-serviceprovider-overview {
    margin-bottom: 30px;
    white-space: pre-wrap;
}

.cb-serviceprovider-contentdisclaimer {
    display: block;
    margin-bottom: 10px;
    font-size: 10px;
    opacity: 0.4;
}

.cb-serviceprovider-priceDetail {
    line-height: 50px;
    font-weight: Bold;
    font-size: 18px;
}

.cb-serviceprovider-price {
    color: #B12704;
    font-size: 18px;
}

.cb-serviceprovider-priceAttribute {
    margin-bottom: 36px;
    font-weight: Bold;
}

.cb-serviceprovider-tr {
    font-size: 15px;
    height: 55px;
}

.cb-serviceprovider-td,
.cb-serviceprovider-colon {
    padding-right: 10px;
}

.cb-serviceprovider-colon {
    font-weight: Bold;
}

.cb-select-box select {
    appearance: none;
    -webkit-appearance: none;
    -moz-appearance: none;
    background: transparent url(../img/caret.png) no-repeat scroll 95% 50%/1.5rem 1rem;
    border-color: #C0C0C0 #D9D9D9 #D9D9D9;
    border-radius: 0;
    border-style: solid;
    border-width: 1px;
    display: block;
    padding: 0 3rem 0 1rem;
    position: relative;
    width: auto !important;
    height: 34px;
}

/* 撰写评论 */

@media (min-width: 1200px) {
    .col-lg-offset-3 {
        margin-left: 25%;
    }
}

@media (min-width: 992px) {
    .col-md-3 {
        width: 25%;
    }
}

@media (min-width: 1200px) {
    .col-lg-9 {
        width: 75%;
    }
}

.cb-serviceprovider-writeReview {
    margin: 20px 0 40px;
}

h4 {
    font-size: 18px;
    margin-top: 10px;
    margin-bottom: 10px;
}

.cb-serviceprovider-bigLink {
    font-size: 20px;
    font-weight: normal;
    color: 0097d6;
}

.cb-serviceprovider-reviewGrid {
    margin-bottom: 20px;
    margin-left: 5px;
}

.cb-serviceprovider-reviewNav {
    border-bottom: 1px solid #c0c0c0;
    font-size: 18px;
    font-weight: 700;
    padding-bottom: 5px;
    margin-bottom: 20px;
}

.cb-serviceprovider-reviewNav .tab {
    display: inline-block;
    color: #c0c0c0;
    height: 25px;
    padding-right: 40px;
}

.cb-serviceprovider-reviewNav .tab>span {
    display: block;
    padding-bottom: 2px;
}

.border_bottom7 {
    border-bottom: 7px solid #c0c0c0;
}

.cb-serviceprovider-reviewNav a {
    color: #000000;
    padding-right: 5px;
    background-color: transparent;
    cursor: pointer;
}

.cb-serviceprovider-reviewItem {
    font-size: 14px;
    margin-bottom: 30px;
}

.cb-serviceprovider-reviewItem .rating-pane {
    padding: 0;
    padding-right: 40px;
}

.cb-searchresults-esp-rating {
    font-size: 15px;
    font-weight: 400;
    display: block;
    color: #0097d6;
}

.rating-pane .cb-searchresults-esp-rating>div {
    margin: 5px 0;
    color: #000;
}

.rating-star-5 {
    background-position: -5px -368px;
}

.rating-icon-star {
    background-image: url(../img/aui_sprite_0037-1x._V1_.png);
    background-repeat: no-repeat;
    background-size: 400px 650px;
    display: inline-block;
    position: relative;
    vertical-align: middle;
    height: 18px;
    width: 80px;
}
.cb-searchresults-esp-rating .date {
    color: #000;
    font-size: 14px;
    opacity: 0.4;
}
.cb-serviceprovider-reviewItem .title {
    margin-bottom: 10px;
}
