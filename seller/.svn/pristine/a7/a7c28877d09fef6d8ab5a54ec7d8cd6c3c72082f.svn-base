.content-frame {
    padding: 14px 18px 0px;
    margin: 15px auto;
    text-align: left;
}
.madagascar-navigation {
    padding: 14px 18px 0px;
}
.spacing-medium {
    margin-bottom: 18px;
}
.unordered-list {
    margin-left: -20px;
}
.unordered-list li {
    width: 100px;
    text-align: center;
    border-right: 1px solid #DDDDDD;
    float: left;
    line-height: 15px;
}
.content-frame h1 {
    font-size: 28px;
    font-weight: normal;
    line-height: 1.2;
    color: #c60;
}
.nav{
    overflow: hidden;
}
.nav li{
    float: left;
    padding:10px;
}