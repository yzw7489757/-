/* 气泡 */

.a-popover-wrapper {
    border: 1px solid #cdcdcd;
    border-color: rgba(0, 0, 0, .2);
    height: 100%;
    width: 100%;
    border-radius: 4px;
    box-shadow: 0 2px 4px rgba(0, 0, 0, .13);
    background-color: #fff;
}

.a-popover-inner2 {
    background-color: #fff;
    padding: 14px 18px;
    text-align: left;
    overflow-x: hidden;
}

.a-popover-inner .a-button-close {
    position: static;
    margin: -10px -14px 6px 6px;
}

.a-button-close {
    display: block;
    zoom: 1;
    background-color: transparent;
    border-radius: 3px;
    border: none;
    float: right;
    padding: 8px;
    position: absolute;
    top: 17%;
    right: 4px;
    margin: -13px 0 0;
    line-height: 0;
}

.a-icon-close {
    opacity: .64;
    filter: alpha(opacity=64);
    width: 15px;
    height: 16px;
    background-position: -297px -5px;
}

.a-arrow-border>span {
    display: block;
    width: 0;
    height: 0;
    border-width: 8px 8px 8px 0;
    border-style: solid;
    border-color: transparent #ccc transparent;
    position: relative;
}

.a-arrow-border>span>em {
    display: block;
    width: 0;
    height: 0;
    border-width: 8px 8px 8px 0;
    border-style: solid;
    border-color: transparent #fff transparent;
    position: absolute;
    top: -8px;
    left: 2px;
}

.a-arrow-border-personal-absolute {
    position: absolute;
    top: 50%;
    left: -7px;
    margin-top: -8px;
}

.a-arrow-border-business-absolute {
    position: absolute;
    top: 230px;
    left: -7px;
}

