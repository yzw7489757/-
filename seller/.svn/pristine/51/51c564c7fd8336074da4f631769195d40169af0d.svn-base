/* 上下三角形 */

.bank-account-dropdown:before {
    top: 8px;
    border-color: transparent transparent #000 transparent !important;
}

.bank-account-dropdown:before,
.bank-account-dropdown:after {
    content: '';
    position: absolute;
    z-index: 2;
    right: 10px;
    width: 0;
    height: 0;
    border: 3px dashed;
    pointer-events: none;
}

.bank-account-dropdown:after {
    /* margin-top: 5px; */
    border-top-style: solid;
    border-bottom: none;
}

.bank-account-dropdown:after {
    top: 16px;
    border-color: #000 transparent transparent !important;
}

.bank-account-dropdown:before,
.bank-account-dropdown:after {
    content: '';
    position: absolute;
    z-index: 2;
    right: 10px;
    width: 0;
    height: 0;
