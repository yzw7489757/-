.data-analysis-conatiner{
	width: 100%;
	padding: 20px 2.5%;
}
.a-horizontal-stripes {
    border: 1px solid #e7e7e7;
    line-height: 1.2;
    font-size: 12px;
    min-width: 100%;
}
table.a-horizontal-stripes tr:first-child th {
    background: -webkit-linear-gradient(top,#f8f8f8,#eee);
    background: linear-gradient(to bottom,#f8f8f8,#eee);
    box-shadow: 0 1px 0 rgba(255,255,255,.5) inset;
    border-color: #e7e7e7;
    
}
.a-horizontal-stripes td, .a-horizontal-stripes th {
    height: auto !important;
    max-width: 200px;
    text-align: center;
    padding: 10px 5px!important;
    border-bottom: 1px solid #ddd;
}
.a-button {
    background: #e7e9ec;
    border-radius: 3px;
    border-color: #ADB1B8 #A2A6AC #8D9096;
    border-style: solid;
    border-width: 1px;
    cursor: pointer;
    display: inline-block;
    padding: 0;
    text-align: center;
    text-decoration: none!important;
    vertical-align: middle;
}
.a-button-dark, .a-button-search {
    background: #444C55;
    border-color: #3d444c #2f353b #2c3137;
    color: #fff;
}
.a-button-inner {
    display: block;
    position: relative;
    overflow: hidden;
    height: 29px;
    box-shadow: 0 1px 0 rgba(255,255,255,.6) inset;
    border-radius: 2px;
}
.a-button-dark .a-button-inner, .a-button-search .a-button-inner {
    background: #5b626a;
    background: -webkit-linear-gradient(top,#72787F,#444C55);
    background: linear-gradient(to bottom,#72787F,#444C55);
    box-shadow: 0 1px 0 rgba(255,255,255,.15) inset;
    text-shadow: 0 -1px 0 rgba(0,0,0,.25);
}
.a-button-text {
    background-color: transparent;
    border: 0;
    display: block;
    font-size: 13px;
    line-height: 29px;
    margin: 0;
    outline: 0;
    padding: 0 10px 0 11px;
    text-align: center;
    white-space: nowrap;
}
.a-button-dark .a-button-text, .a-button-search .a-button-text {
    font-weight: 700;
    color: #fff;
}
#selectday{
	border: 1px solid #a6a6a6;
    border-top-color: #949494;
    border-radius: 3px;
    box-shadow: 0 1px 0 rgba(255,255,255,.5), 0 1px 0 rgba(0,0,0,.07) inset;
    background-color: #fff;
    height: 31px;
    padding: 3px 7px;
    width: 150px;
    margin-right: 8px;
    margin-bottom: 30px;
}
.dowloadlink{
	 background: none;
	 margin-left: 10px;
	 cursor: pointer;
	 line-height: 34px;
	 display: inline-block;
	 color: #337ab7;
}
 