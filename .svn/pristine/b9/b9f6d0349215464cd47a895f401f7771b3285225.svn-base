/*二级导航栏*/
#nav-subnav{
    height: 50px;
    background: #fff;
    box-shadow: 0 0 1px #ddd;
    border-top: 1px solid rgba(0,0,0,.17);
    border-bottom: 1px solid rgba(0,0,0,.1);
}
#nav-subnav .nav-li{
    height:100%;
    float: left;
    padding:0 2%;
    line-height: 50px;
    text-align: center;
}
@media screen and (max-width: 1440px) {
    #nav-subnav .nav-li{
        padding:0 1.8%;
    }
}
@media screen and (max-width: 1350px) {
    #nav-subnav .nav-li{
        padding:0 1.5%;
    }
}
@media screen and (max-width: 1230px) {
    #nav-subnav .nav-li{
        padding:0 1.3%;
    }
}
#nav-subnav .nav-li .subnav-a{
    display: block;
    color:#333;
}
#nav-subnav .nav-li .subnav-a:hover .subnav-span{
    border-bottom: 2px solid #f90;
}
#nav-subnav .nav-li:first-child .subnav-a{
    font-weight: 700;
}
