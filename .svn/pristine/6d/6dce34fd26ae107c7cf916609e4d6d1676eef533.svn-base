/*浏览历史记录 historial*/
#nav-flyout-timeline{
    width: 100%;
    border-right: 0px;
    border-left: 0px;
    padding-bottom: 7px;
    position:absolute;
    top:161px;
    left: 0;
    padding-left: 0;
    padding-right: 0;
    min-width: 1000px;
}
.nav-flyout{
    border: 1px solid #bbb;
    padding-bottom: 7px;
    -moz-box-shadow: 0 2px 4px 0 rgba(0,0,0,.13);
    box-shadow: 0 2px 4px 0 rgba(0,0,0,.13);
    padding: 14px;
    z-index: 100;
    background: #fff;
    top: 157px;
    border-radius: 3px;
    font-family: arial,sans-serif;
    display:none;
}
.nav-flyout .nav-arrow ,.nav-flyout .nav-arrow-inner{
    border-style: solid;
    border-width: 9px;
    border-color: transparent;
    border-top-width: 0;
    border-bottom: 9px solid #ccc;
    width: 0;
    height: 0;
    font-size: 0;
    line-height: 0;
    position: absolute;
    top: -9px;
    left:569px;
}
.nav-flyout .nav-arrow-inner {
    border-bottom: 9px solid #fff;
    top: 1px;
    left: -9px;
}
#nav-flyout-timeline .nav-timeline-enabled{
