/*广告栏*/
#nav-ad{
    background-image: url(../img/nav_latest_1x._CB474855083_.jpg);
    background-position: top left;
    height: 55px;
    background-repeat: no-repeat;
    border-top: 1px solid #ffa724;
}
#nav-ad a{
    display: block;
    height: 55px;
}
/*头部搜索栏*/
#nav-belt{
    padding-bottom: 5px;
}
.navbar{
    background-color: #232f3e;
    font-family: arial,sans-serif;
    font-size: 12px;
    line-height: 1em;
}
.nav-logo{
    width: 192px;
    padding: 13px 18px 0 27px;
    position:relative;
}
.nav-logo .nav-logo-link{
    display: inline-block;
}
.navbar .nav-sprite, .navbar .nav-icon,#navFooter .nav-sprite {
    background-image: url(../img/nav-sprite-global_bluebeacon-V3-1x_optimized._CB474516462_.png);
    background-position: 0 1000px;
    background-repeat: repeat-x;
}
.nav-logo .nav-logo-base {
    background-position: -10px -51px;
    width: 97px;
    height: 30px;
    display:inline-block;
}
.nav-logo .nav-logo-tagline{
    color: #48a3c6;
    position: absolute;
    top: 31px;
    left: 93px;
    font-size: 11px;
    line-height: 11px;
    white-space: nowrap;
    text-decoration: underline;
    font-weight: 700;
}
.nav-logo .nav-logo-tagline:hover{
    color: #fff;
}
.nav-right{
    position:relative;
}
.nav-right .nav-swmslot{
    position: relative;
    overflow: hidden;
    height: 39px;
    top: 9px;
    width: 400px;
}
.nav-right .nav-swmslot .nav-imageHref{
    display:block;
    height: 100%;
}
#nav-search{
    padding-top: 11px;
    padding-right: 25px;
    white-space: nowrap;
    overflow: hidden;
}
#nav-search .nav-searchbar{
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    position: relative;
    display: block;
}
#nav-search .nav-searchbar.active{
    -moz-box-shadow: 0 0 3px rgba(228,121,17,.5),0 1px 0 rgba(0,0,0,.07) inset;
    box-shadow: 0 0 3px rgba(228,121,17,.5), 0 1px 0 rgba(0,0,0,.07) inset;
}
.nav-searchbar .search-left,.nav-searchbar .search-right{
    position: relative;
    height: 35px;
}
#nav-search .nav-search-scope{
    background-color: #f3f3f3;
    border: 1px solid #f3f3f3;
    border-right: 1px solid #cdcdcd;
    -moz-border-radius: 4px 0 0 4px;
    border-radius: 4px 0 0 4px;
    padding-left: 5px;
    cursor: pointer;
}
#nav-search .nav-search-scope:hover{
    background-color: #dadada;
    border-left-color: #dadada;
    border-top-color: #dadada;
    border-bottom-color: #dadada;
}
.nav-searchbar .nav-search-facade{
    position: relative;
    overflow: hidden;
    top: 1px;
}
#nav-search .nav-search-facade .nav-search-label {
    display: block;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    color: #666;
    font-size: 12px;
    line-height: 33px;
    margin-right: 21px;
    margin-left: 5px;
}
#nav-search .nav-search-facade .nav-icon {
    position: absolute;
    top: 14px;
    right: 8px;
    border-style: solid;
    border-width: 4px;
    border-color: transparent;
    border-top: 4px solid #666;
    border-bottom-width: 0;
    width: 0;
    height: 0;
    font-size: 0;
    line-height: 0;
}
#nav-search .nav-search-dropdown {
    position: absolute;
    display: block;
    top: -1px;
    left: 0;
    height: 35px;
    width: auto;
    outline: 0;
    margin: 0;
    padding: 0;
    cursor: pointer;
    opacity: 0;
    filter: alpha(opacity=0);
    border: 0;
    line-height: 35px;
}
.nav-searchbar .search-right .nav-search-submit{
    background-color: #febd69;
    width: 45px;
    position: relative;
    top: 0;
    right: 0;
    height: 35px;
    overflow: hidden;
    cursor: pointer;
    -webkit-border-radius: 0 4px 4px 0;
    -moz-border-radius: 0 4px 4px 0;
    border-radius: 0 4px 4px 0;
}
#nav-search .nav-search-submit:hover {
    background-color: #f3a847;
}
#nav-search-submit-text{
    background-position: -10px -290px;
    width: 21px;
    height: 21px;
    position:absolute;
    top: 7px;
    left: 12px;
}
#nav-search .nav-search-submit .nav-input {
    position: relative;
    display: block;
    height: 100%;
    width: 100%;
    cursor: pointer;
    background: 0 0;
}
#nav-search .search-fill{
    overflow: hidden;
    height: 35px;
    position: relative;
    background: #fff;
}
#nav-search .search-fill .search-input{
    position: absolute;
    top: 0;
    left: 0;
    display: block;
    height: 100%;
    line-height: 15px;
    padding: 6px 10px 6px 0;
    font-size: 15px;
    color: #111;
    text-indent: 8px;
    width: 100%;
    border:0;
    box-shadow: none;
}
#nav-main .nav-left,#nav-main .nav-right,#nav-main .nav-fill{
    height: 55px;
}
#nav-main .nav-fill{
    overflow: hidden;
    white-space: nowrap;
}
