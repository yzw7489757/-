.sc-one-click-divider{
    position: relative;
    top: 2px;
    padding-top: 1px;
    margin-bottom: 14px;
    text-align: center;
}
.sc-one-click-divider:after{
    content: "";
    width: 100%;
    background-color: transparent;
    display: block;
    height: 1px;
    border-top: 1px solid #e7e7e7;
    position: absolute;
    top: 50%;
    margin-top: -1px;
    z-index: 1;
}
.sc-one-click-divider h5{
    display:inline-block;
    position:relative;
    z-index: 2;
    font-size: 12px;
    color: #767676;
    font-weight: 400;
    padding: 0 8px 0 7px;
    background-color: #f3f3f3;
}
.shopping_cart_left{
    padding-right: 3.5%;
    overflow: hidden;
}
#sc-new-upsell{
