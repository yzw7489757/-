}
a:hover{
    text-decoration: underline;
    color: #c45500;
}
.a-box{
    border: 1px #ddd solid;
    margin-top: -1px;
}
#sc-buy-box .a-box:first-child, #sc-buy-box .a-box:first-child .a-box-inner{
    border-radius: 4px 4px 0 0;
}
#sc-buy-box .a-box:last-child, #sc-buy-box .a-box:last-child .a-box-inner{
    border-radius: 0 0 4px 4px;
}
.a-box .a-box-inner {
    padding: 14px 18px;
}
.a-alert-inline-success .a-alert-container{
    padding-left: 22px;
    color: #007600;
    position: relative;
}
.a-alert-inline-success .a-alert-container .a-icon-alert{
    background-position: -117px -130px;
    height: 13px;
    width: 14px;
    position: absolute;
    left: 2px;
    top: 2px;
}
.qualifies-content{
