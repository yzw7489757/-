.sc-nav-arrow{
    display: block;
    position: absolute;
    margin: 0;
    left: 50%;
    margin-left: -6px;
    bottom: 0;
    height: 0;
    padding: 0;
    border-left: 6px solid transparent;
    border-right: 6px solid transparent;
    border-bottom: 6px solid #333;
    z-index: 102;
    opacity: 0;
    visibility: hidden;
}
.sc-nav-arrow-right{
    display: block;
    position: absolute;
    margin: 0;
    right: 6px;
    top: 29px;
