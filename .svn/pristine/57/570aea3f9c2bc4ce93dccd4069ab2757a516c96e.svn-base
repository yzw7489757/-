#spaui{
    height:100vh;
    width: 410px;
    position: fixed;
    margin:0;
    top:0;
    bottom: 0;
    right: -410px;
    background:lightpink;
    z-index: 20000;
    list-style: none;
    text-align: left;
    background-image: none;
    color: #333;
    font-size: 13px;
    transition-property: width,right;
    transition-duration: 1s;
    -webkit-ttransition-property: width,right;
    -webkit-ttransition-duration: 1s;
    -o-ttransition-property: width,right;
    -o-ttransition-duration: 1s;
    -moz-ttransition-property: width,right;
	-moz-ttransition-duration: 1s;
    
}
