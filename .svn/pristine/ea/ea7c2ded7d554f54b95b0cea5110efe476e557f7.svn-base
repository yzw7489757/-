}
.nav_tab{
    background: -webkit-linear-gradient(top, #f8f8f8, #eeeeee);
    background: -o-linear-gradient(top, #f8f8f8, #eeeeee);
    background: -ms-linear-gradient(top, #f8f8f8, #eeeeee);
    background: -moz-linear-gradient(top, #f8f8f8, #eeeeee);
    box-shadow: 0 1px 0px rgba(255, 255, 255, 0.5) inset;
    border: 1px solid #dddddd;
}
.tab{
    text-decoration: none;
    line-height: 2;
    padding: 2px 17px 0px 18px;
    border: 1px solid transparent;
    border-bottom: 0;
    float: left;
    margin-bottom: -1px;
    margin-top: -1px;
    color: #004b91;
    cursor: pointer;
}
.tab:first-child{
    border-left: 0;
}
.notification-required .icon{
    background-image: url(../img/AmazonUIBaseCSS-sprite_1x-28bd59af93d9b1c745bb0aca4de58763b54df7cf._V2_.png);
    background-position: -249px -30px;
    display: inline-block;
    width: 28px;
    height: 31px;
    position:relative;
    top: 6px;
    opacity: .4;
}
.tab .text{
    display:inline-block;
    line-height: 52px;
}
.tab.active{
    border-color: #dddddd;
    background: #fff;
    border-top-color: #e47911;
    font-weight: bold;
    color: #e47911;
}
.active .icon{
    opacity:1;
}
.tab:hover{
    border-top-color: #e47911;
    color: #e47911;
}
.view-toggle-tab-bar{
    padding: 15px 17px 13px 18px;
}
.aui-switch > .toggle-input {
    position: absolute;
    visibility: hidden;
}
.aui-switch > .switch-background {
    position: relative;
    cursor: pointer;
    outline: none;
    user-select: none;
    border: 1px solid;
    border-color: #8D9096 #ADB1B8 #ADB1B8;
    height: 24px;
    width: 28px;
    background-color: #F0F0F0;
    transition: background 0.6s;
    vertical-align: middle;
    display: inline-block;
    border-radius: 3px;
}
.aui-switch > input.toggle-input:checked + .switch-text + .switch-background {
    background: #e47911;
}
.aui-switch .switch-control {
    position: absolute;
    left: 0;
    background: linear-gradient(#EFF1F3, white);
    height: 22px;
    width: 16px;
    border-width: 1px;
    border-style: solid;
    border-color: #ADB1B8 #A2A6AC #8D9096;
    display: inline-block;
    border-radius: 3px;
    box-shadow: white 0 1px 0 0 inset, rgba(0, 0, 0, 0.0980392) 0 1px 3px inset, white 0 0 1px inset, rgba(0, 0, 0, 0.14902) 0 1px 2px;
    transition: margin 0.3s;
}
.aui-switch > input.toggle-input:checked + .switch-text + .switch-background > .switch-control {
    margin-left: 10px;
}
.breadcrumb {
    padding: 14px 18px;
    border-left: 1px solid #ddd;
