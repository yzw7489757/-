body{
    font-family: verdana,arial,helvetica,sans-serif;
    font-size: 12px;
    background: #fff;
    color: #000;
}
input[type=text],input[type=password]{
    height: 33px;
}
a:link {
    color: #004b91;
}
a:hover {
    color: #E47911 !important;
    text-decoration: underline; 
}
.header{
    min-width: 1060px;
    width:67%;
    padding: 20px 0;
    margin: 0 auto;
}
.header .logo{
    display: inline-block;
}
.container{
     min-width: 1060px;
     width:67%;
